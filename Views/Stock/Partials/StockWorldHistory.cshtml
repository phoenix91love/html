
<div class="row">
    <div class="col-lg-12">
        <form>
            <input type="hidden" id="hd-tab" value="1" />
            <div class="form-group row">
                <label for="input-from-date" class="col-lg-1 col-sm-12 col-form-label">Từ ngày</label>
                <div class="col-lg-2 col-sm-12">
                    <input type="text" class="form-control" id="input-from-date" readonly placeholder="Từ ngày" value="@ViewData["FromDate"]">
                </div>
                <label for="input-to-date" class="col-lg-1 col-sm-12 col-form-label">Đến ngày</label>
                <div class="col-lg-2 col-sm-12">
                    <input type="text" class="form-control" id="input-to-date" readonly placeholder="Đến ngày" value="@ViewData["ToDate"]">
                </div>
                <div class="col-lg-1 col-sm-3">
                    <button type="button" class="btn btn-blue btn-search">Tìm kiếm</button>
                </div>
            </div>
            <div class="form-group row">

                <label for="input-code" class="col-lg-1 col-sm-12 col-form-label">Mã CK</label>
                <div class="col-lg-2 col-sm-12">
                    <input type="text" class="form-control" id="input-code" placeholder="Mã chứng khoán">
                </div>

            </div>
        </form>
    </div>
</div>
<div class="row">
    <div class="col-lg-12" id="world-history-grid">
    </div>
</div>
